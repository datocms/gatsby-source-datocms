{
  "name": "gatsby-source-datocms",
  "version": "5.0.1",
  "description": "Gatsby source plugin for building websites using DatoCMS as data source",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/datocms/gatsby-source-datocms.git"
  },
  "scripts": {
    "prepare": "husky install",
    "prettier": "prettier --write README.md \"src/**/*.{js,jsx}\"",
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production yarn build",
    "test-v4": "jest --forceExit",
    "test": "jest --forceExit --testNamePattern='^(?!tracedSVG)'"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/preset-env": "^7.6.2",
    "cross-env": "^7.0.3",
    "datocms-structured-text-to-html-string": "^1.0.13",
    "gatsby": "^4.24.0 || ^5.0.0",
    "gatsby-plugin-image": "^2.0.0 || ^3.0.0",
    "gatsby-plugin-sharp": "^4.0.0 || ^5.0.0",
    "gatsby-transformer-remark": "^5.0.0 || ^6.0.0",
    "husky": "^7.0.4",
    "jest": "^28.1.1",
    "np": "^7.4.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^3.1.1",
    "react": "^16.10.2",
    "react-helmet": "^5.2.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "datocms-client": "^3.5.16",
    "datocms-structured-text-utils": "^1.0.13",
    "gatsby-core-utils": "^3.1.0",
    "gatsby-plugin-utils": "^2.0.0",
    "humps": ">=2.0.1",
    "imgix-url-params": ">=11.11.2",
    "json-stringify-safe": ">=5.0.1",
    "lodash.uniq": "^4.5.0",
    "md5": "^2.3.0",
    "object-assign": "^4.1.1",
    "object.entries": "^1.1.3",
    "promise-queue": "2.2.5",
    "query-string": "6.14.1",
    "regenerator-runtime": "^0.13.3",
    "semver": "^7.3.5",
    "unist-util-visit": "^2.0.3"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "author": "DatoCMS <support@datocms.com>",
  "license": "ISC",
  "peerDependencies": {
    "gatsby": ">= 4.24.0",
    "gatsby-plugin-image": ">= 1.0.0",
    "react": "*",
    "react-helmet": ">= 5.0.0"
  },
  "peerDependenciesMeta": {
    "gatsby-plugin-image": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.x"
  }
}
