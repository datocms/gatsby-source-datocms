{
  "name": "gatsby-source-datocms",
  "version": "2.5.18",
  "description": "Gatsby source plugin for building websites using DatoCMS as data source",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/datocms/gatsby-source-datocms.git"
  },
  "scripts": {
    "prettier": "prettier --write \"src/**/*.{js,jsx}\"",
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "test": "yarn build && uvu test '.test.js'"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/preset-env": "^7.6.2",
    "cross-env": "^6.0.3",
    "datocms-structured-text-to-html-string": "^1.0.12",
    "gatsby": "^2.24.63",
    "gatsby-plugin-image": "^1.1.0-next.2",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-transformer-remark": "^2.8.38",
    "np": "^7.4.0",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-helmet": "^5.2.0",
    "rimraf": "^3.0.2",
    "uvu": "^0.3.3"
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "datocms-client": ">=3.3.0",
    "datocms-structured-text-utils": "^1.0.12",
    "fs-extra": ">=8.1.0",
    "got": "^11.8.1",
    "humps": ">=2.0.1",
    "imgix-url-params": ">=11.11.2",
    "json-stringify-safe": ">=5.0.1",
    "lodash.uniq": "^4.5.0",
    "md5": "2.2.1",
    "object-assign": "^4.1.1",
    "object.entries": "^1.1.0",
    "promise-queue": "2.2.5",
    "query-string": "6.8.3",
    "regenerator-runtime": "^0.13.3",
    "unist-util-visit": "^2.0.3"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "author": "DatoCMS <support@datocms.com>",
  "license": "ISC",
  "peerDependencies": {
    "gatsby": "^2.12.1",
    "gatsby-plugin-image": "^1.0.0",
    "react": "*",
    "react-helmet": ">= 5.0.0"
  }
}
